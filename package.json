{
  "name": "website-cass",
  "description": "Portfolio website of writer Cassandra Frances",
  "version": "0.0.1",
  "author": "Axel Bocciarelli",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/axelboc/website-cass"
  },
  "homepage": "https://cassfrances.com",
  "engines": {
    "node": "^6.5.0",
    "npm": "^3.10.3"
  },
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "start": "phenomic start",
    "build": "phenomic build",
    "pretest": "npm run lint",
    "test": "npm run build"
  },
  "dependencies": {
    "normalize.css": "~4.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-react-jsx-self": "^6.11.0",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.13.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.0",
    "eslint": "^3.7.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^6.4.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.0",
    "json-loader": "^0.5.4",
    "markdown-it-front-matter-loader": "0.0.7",
    "npm-run-all": "^1.7.0",
    "phenomic": "^0.20.4",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react": "^15.0.0-rc.1",
    "react-dom": "^15.0.0-rc.1",
    "react-helmet": "^3.0.0",
    "react-hot-loader": "^3.0.0-beta",
    "react-redux": "^4.0.0",
    "react-render-html": "^0.1.6",
    "react-router": "^2.3.0",
    "react-svg-inline": "^1.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "redux": "^3.0.0",
    "style-loader": "^0.16.1",
    "stylelint": "^7.2.0",
    "stylelint-config-css-modules": "^0.1.0",
    "webpack": "^2.3.2",
    "whatwg-fetch": "^0.11.0",
    "yaml-front-matter": "^3.4.0"
  },
  "phenomic": {
    "nojekyll": false
  },
  "#babel": "webpack-(development|production) are useful for webpack 2, otherwise use development|production",
  "babel": {
    "env": {
      "test": {
        "presets": [
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      },
      "development": {
        "presets": [
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-react-jsx-source",
          "babel-plugin-transform-react-jsx-self",
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          "babel-preset-react-optimize",
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      },
      "webpack-development": {
        "presets": [
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-stage-1",
          "babel-preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-react-jsx-source",
          "babel-plugin-transform-react-jsx-self",
          "react-hot-loader/babel"
        ]
      },
      "webpack-production": {
        "presets": [
          "babel-preset-react-optimize",
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": "./node_modules/phenomic/lib/eslint-config-recommended/index.js",
    "rules": {
      "semi": 2,
      "no-console": 0
    }
  },
  "stylelint": {
    "extends": [
      "./node_modules/phenomic/lib/stylelint-config-recommended/index.js",
      "stylelint-config-css-modules"
    ]
  }
}
