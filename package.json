{
  "name": "website-cass",
  "description": "Portfolio website of writer Cassandra Frances",
  "version": "0.0.1",
  "author": "Axel Bocciarelli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/axelboc/website-cass"
  },
  "scripts": {
    "build": "brunch build",
    "clean": "rm -Rf public",
    "prod": "brunch build --production",
    "serve": "browser-sync start --server public --files public --extensions html",
    "start": "npm run clean && concurrently \"npm run watch\" \"npm run serve\"",
    "watch": "brunch watch"
  },
  "dependencies": {
    "normalize.css": "~4.2.0"
  },
  "devDependencies": {
    "browser-sync": "^2.14.0",
    "brunch": "~1.8.5",
    "brunch-static": "~1.2.1",
    "concurrently": "^2.2.0",
    "cssnano": "~3.7.3",
    "html-brunch-static": "~1.3.1",
    "postcss-brunch": "~0.5.0",
    "postcss-custom-properties": "~5.0.1",
    "postcss-import": "~8.1.2",
    "postcss-nested": "~1.0.0",
    "watch": "~0.19.1"
  }
}
